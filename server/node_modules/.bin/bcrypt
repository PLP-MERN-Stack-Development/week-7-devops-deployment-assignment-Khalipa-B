#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules:/home/khalipa/Desktop/PLP_MERN/week7/week-7-devops-deployment-assignment-Khalipa-B/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/bcrypt" "$@"
else
  exec node  "$basedir/../.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/bcrypt" "$@"
fi
